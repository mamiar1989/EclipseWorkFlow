
public class LearnDatatype {

	
// Premitive data typ
		int age=25;
		byte ram=20;
		short number=500;
		long ssn= 458752361785L;
		double salary= 30.5555555;
		float coursefee=5000.00f;
		
		
	// non premitive datatype	
		String name="Maria   sharapova ";
		
		public static String  mobileBrand = "Iphone"; //this a static variable created to be used in another class 
		// created inside another class , when it is public we can use it anywhere outside or inside a project.
		
		public static void main(String[] args) {
			
			int num1=25;
			int num2=56;		
			int total= num1+num2;
			System.out.println(total);
			
		
		
		
	}

}
