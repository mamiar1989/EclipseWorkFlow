
public class Casting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		double price = 35.99;
		int discountePrice = (int)price;
		System.out.println (discountePrice);
		
		
		int num1 = 60;
		int num2 = 40;
		int num3 =num1;
		int num4 = num3+ num2;
		System.out.println("total is" + num4);
		
		
		
		
		// 
		
		
		
		
		
		
		
		
		
		
		
	}

}
