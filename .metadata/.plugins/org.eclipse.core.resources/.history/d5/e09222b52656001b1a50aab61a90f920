package javapractice;

public class Learn_ObjectAndClass2 { 
	
		
		int high;
		int weight;
		int age;
		double width;
	
	public static void main(String[] args) {
		
		
	Learn_ObjectAndClass2 obj = new Learn_ObjectAndClass2();
	obj.age=30;
	obj.weight=500;
	obj.high=20;
	obj.width = 30;
	System.out.println( " the age of my oak tree is " + " " + obj.age + " " +"years old"  );
	System.out.println();	
	System.out.println();
	System.out.println();
		
		
		 
	}
	
	
	
	
	
}

		
		

