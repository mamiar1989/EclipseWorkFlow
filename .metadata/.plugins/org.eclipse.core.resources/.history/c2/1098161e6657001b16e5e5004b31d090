package javapractice;

public class Student {
	
	int rno;
    String name;
	public Student() {
		name = "ABC";
		rno=123;
		}

	public static void main(String[] args) {
		
		Student S1 = new Student();
		S1.name="ABC";
		S1.rno=123;
		System.out.println(S1.name);
		System.out.println(S1.rno);
		
		
		
		
		
		
	}
		
		
		
		
		
	}

}
