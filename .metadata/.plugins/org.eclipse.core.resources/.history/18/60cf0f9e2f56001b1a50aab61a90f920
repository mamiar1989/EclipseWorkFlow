		// classname object name= new constractorOfClass();
		
		
		
public Constructor () {} // this constructor
System.out
// Without parameter its called default Constructor
		
		// this is the DefaultConstractor
public static void main (String[] args) {
Constractor learn = new Constractor ();
}
			

			
		// Signature Pattern 
		// Create Parameter
  public Constructor(String name) {
		// with parameter its called parameterized constructor 
    	 
	}
	
	
	
	

