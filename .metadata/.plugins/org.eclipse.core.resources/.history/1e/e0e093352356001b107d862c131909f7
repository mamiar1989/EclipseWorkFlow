package javapractice;

public class Learn_ObjectAndClass2 {

class Student{
	int Age;
	int IdNumber;
	public static String language= "french"; 
}
	
	
   
	public static void main(String[] args) {
		
		Student Maria= new Student ();
		
		Maria.Age= 30;
		Maria.IdNumber=85325;
	
		System.out.println("Maria's age is :" + " " + Maria.Age + " " + "years old" );
		System.out.println(" Maria's ID Number is " + Maria.IdNumber );
		String Language;
		System.out.println( "Maria's language is " + " " + Language);
	}

}
 

	
		  
		  
		  
		  
	  }
	  }
	
	
	
	
	
	
	
	
}