package LearnConstructors;

public class Constructor {

		// Constructor
		// Constructor name should be same as class name
		//Constructor has no return type 
		//Constructor are used to initialize object 
		// AccessModifier classNme () {}
		// class name object name= new constractorOfClass();
	  
	public Constructor () {
		System.out.println("this is default constractur"); 
		System.out.println("this is default constractur"); 
		System.out.println("this is default constractur"); 
		System.out.println("this is default constractur"); 
		System.out.println("this is default constractur"); 
		int num1=50;
		int num2=60;
		int total= num1+num2;		
		System.out.println(total);
	  } // default parameter this constructor
		// when there is no arguments it is called default parameter
		
	

		// Without parameter its called default Constructor
		
		// this is the DefaultConstractor
		public static void main (String[] args) {
		Constructor learn = new Constructor ();
}
			
		// Signature Pattern by creating parameter
		// Create Parameter
		public Constructor(String name) {// we add string name to five some parameters
		// with parameter its called parameterized constructor 
    	 
	}
	
	
	
	

}
